<ul class="feeds">
	{% for task in family.tasks %}
		<li>
			<div class="col1">
				<div class="cont">
					<div class="cont-col1">
						<div class="label label-info">
							{{ task.createdAt|date("Y-m-d") }}
						</div>
					</div>
					<div class="cont-col2">
						<div class="desc">
							{{ task.message }}
						</div>
					</div>
				</div>
			</div>
			<div class="col2">
				<div class="date">
					<a href="">
						<span class="label label-sm label-warning">Pending</span>
					</a>
				</div>
			</div>
		</li>
	{% endfor %}
</ul>
